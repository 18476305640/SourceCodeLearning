<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--这个bean将进行自动注入-->
    <bean id="book" class="com.scl.spring._2_ioc管理bean._1_xml方式._2_注入属性._自动注入.Book">
        <property name="name" value="西游记"></property>
        <property name="author" value="吴承恩"></property>
    </bean>
    <!--会自动注入属性-->
    <bean id="user" class="com.scl.spring._2_ioc管理bean._1_xml方式._2_注入属性._自动注入.User" autowire="byName">
        <property name="id" value="19060201111" ></property>
    </bean>
</beans>